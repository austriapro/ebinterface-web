<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">
	<appender class="ch.qos.logback.core.ConsoleAppender" name="console">
		<encoder>
			<pattern>%-5p %c{2} %m %throwable %n</pattern>
		</encoder>
	</appender>
	
	<logger name="at.ebinterface.validation.digitalsignatures" level="debug" />
	<logger name="org.apache.jcp.xml.dsig.internal.dom" level="debug"/>
	
	<root>
		<level value="debug" />
		<appender-ref ref="console" />
	</root>
</configuration>