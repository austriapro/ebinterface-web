<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" metadata-complete="false">
  <display-name>ebInterface Web Application</display-name>
	
	<!-- Wicket part -->
  <filter>
    <filter-name>ValidationService</filter-name>
    <filter-class>org.apache.wicket.protocol.http.WicketFilter</filter-class>
    <init-param>
      <param-name>applicationClassName</param-name>
      <param-value>at.ebinterface.validation.web.ValidationApplication</param-value>
    </init-param>
  </filter>


  <filter-mapping>
    <filter-name>ValidationService</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>


  <session-config>
    <session-timeout>30</session-timeout>
  </session-config>
</web-app>
